ISR(TIMER0_COMPA_vect) {

  //——————- Ringbuffer handler ————————-

  if (RingCount) { //If entry in FIFO..

    OCR1A = Ringbuffer[(RingRead++)]; //Output LSB of 16-bit DAC

    RingCount–;

